{
    "manifest_version": 3,
    "name": "CoCr",
    "description": "A chrome extension to download media allowed via cobalt.tools",
    "version": "1.0",
    "options_page": "options.html",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png",
        "256": "icon256.png",
        "512": "icon512.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon512.png"
    }
  }